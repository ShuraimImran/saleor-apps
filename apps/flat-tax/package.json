{
  "name": "saleor-app-flat-tax",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check-types": "tsc",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "dev:debug": "APP_LOG_LEVEL=debug pnpm dev",
    "e2e": "vitest --project e2e --watch=false --sequence.sequential",
    "e2e:watch": "vitest --project e2e --watch --sequence.sequential",
    "fetch-schema": "curl https://raw.githubusercontent.com/saleor/saleor/${npm_package_saleor_schemaVersion}/saleor/graphql/schema.graphql > graphql/schema.graphql",
    "generate": "pnpm run generate:app && pnpm run generate:e2e",
    "generate:app": "graphql-codegen",
    "generate:e2e": "graphql-codegen --project e2e",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "next start",
    "test": "vitest --project units --sequence.shuffle",
    "test:ci": "vitest run --coverage --project units --sequence.shuffle"
  },
  "dependencies": {
    "@hookform/resolvers": "catalog:",
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/api-logs": "catalog:",
    "@opentelemetry/instrumentation": "catalog:",
    "@opentelemetry/resources": "catalog:",
    "@opentelemetry/sdk-logs": "catalog:",
    "@opentelemetry/sdk-metrics": "catalog:",
    "@opentelemetry/sdk-trace-node": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:",
    "@saleor/app-sdk": "catalog:",
    "@saleor/apps-logger": "workspace:*",
    "@saleor/apps-otel": "workspace:*",
    "@saleor/apps-shared": "workspace:*",
    "@saleor/apps-ui": "workspace:*",
    "@saleor/macaw-ui": "catalog:",
    "@saleor/react-hook-form-macaw": "workspace:*",
    "@saleor/sentry-utils": "workspace:*",
    "@saleor/webhook-utils": "workspace:*",
    "@sentry/cli": "catalog:",
    "@sentry/nextjs": "catalog:",
    "@t3-oss/env-nextjs": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/next": "catalog:",
    "@trpc/react-query": "catalog:",
    "@trpc/server": "catalog:",
    "country-state-city": "3.2.1",
    "jotai": "^2.4.2",
    "neverthrow": "catalog:",
    "next": "catalog:",
    "pg": "^8.13.1",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hook-form": "catalog:",
